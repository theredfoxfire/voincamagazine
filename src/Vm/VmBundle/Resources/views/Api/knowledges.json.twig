{% for url, knowledge in knowledges %}
{% i = 0, count(knowledges), ++i %}
[
    "url":"{{ url }}",
{% for key, value in knowledge %} {% j = 0, count(key), ++j %}
    "{{ key }}":"{% if j == count(key)%} {{ json_encode(value) }}, {% else %} {{ json_encode(value) }}
                 {% endif %}"
{% endfor %}]
{% endfor %}
